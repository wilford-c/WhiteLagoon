@model IEnumerable<WhiteLagoon.Domain.Entities.Villa>

<h1>Villa List</h1>

<table class="table table-boarded table-striped">
	<thead>

		<tr>

			<th>Name</th>
			<th>Price</th>
			<th>Sqft</th>
			<th>Occupancy</th>
        
		</tr>

	</thead>

	<tbody>
		@foreach (var villa in Model)
		{
			<tr>
				<td>@villa.Name</td>
				<td>@villa.Price</td>
				<td>@villa.Sqft</td>
				<td>@villa.Occupancy</td>
			</tr>
		}
	</tbody>

</table>
